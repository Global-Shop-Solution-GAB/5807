	F.Data.DataView.ToDataTable("dtSaveTemp", "dvSaveTemp", "dtSave")
	
	F.Data.DataTable.RemoveColumn("dtSave", "DELETE")
	
	F.Data.DataView.Create("dtSave", "dvSave", 22, "REVIEW_RECORD_TIMESTAMP is null", "")
	F.Data.DataView.SetValue("dtSave", "dvSave", -1, "REVIEW_RECORD_TIMESTAMP", V.Ambient.Now)
	
	F.Data.DataView.SetFilter("dtSave", "dvSave", "REVIEW_APPROVED is null")
	F.Data.DataView.SetValue("dtSave", "dvSave", -1, "REVIEW_APPROVED", False)
	
	F.Data.DataView.SetFilter("dtSave", "dvSave", "")
	F.Data.DataView.ToDataTableDistinct("dtSave", "dvSave", "dtSaveReview", "EXTERNAL_ORDER_NUMBER*!*REVIEW_RECORD_ID*!*REVIEW_RECORD_TIMESTAMP*!*REVIEW_LAST_MODIFIED_TIMESTAMP*!*REVIEW_LAST_MODIFIED_BY*!*REVIEW_APPROVED*!*REVIEW_APPROVED_TIMESTAMP*!*APPROVED_BY")
	F.Data.DataView.Close("dtSave", "dvSave")
	
	F.Data.DataTable.RemoveColumn("dtSave", "REVIEW_RECORD_ID")
	F.Data.DataTable.RemoveColumn("dtSave", "REVIEW_RECORD_TIMESTAMP")
	F.Data.DataTable.RemoveColumn("dtSave", "REVIEW_LAST_MODIFIED_TIMESTAMP")
	F.Data.DataTable.RemoveColumn("dtSave", "REVIEW_LAST_MODIFIED_BY")
	F.Data.DataTable.RemoveColumn("dtSave", "REVIEW_APPROVED")
	F.Data.DataTable.RemoveColumn("dtSave", "REVIEW_APPROVED_TIMESTAMP")
	F.Data.DataTable.RemoveColumn("dtSave", "APPROVED_BY")
	
	V.Local.sMap.Set("EXTERNAL_ORDER_NUMBER@!@ORDER_NO_EXTERNAL*!*TRANSACTION_CODE@!@TRANSACTIONCODE*!*GSS_CUSTOMER_NUMBER@!@GSS_CUSTOMERNO*!*EXTERNAL_CUSTOMER_NUMBER@!@EXT_CUSTOMERNO*!*ORDER_NUMBER@!@ORDERNUMBER*!*ORDER_TYPE@!@ORDERTYPE*!*ORDER_DUE_DATE@!@ORDERDUEDATE*!*ORDER_DATE@!@ORDERDATE*!*HEADER_USER_FIELD_1@!@USERFIELD1HEAD*!*HEADER_USER_FIELD_2@!@USERFIELD2HEAD*!*HEADER_USER_FIELD_3@!@USERFIELD3HEAD*!*HEADER_USER_FIELD_4@!@USERFIELD4HEAD*!*HEADER_USER_FIELD_5@!@USERFIELD5HEAD*!*SHIP_VIA@!@SHIPVIA*!*CARRIER@!@CARRIER*!*SHIP_TO_ID@!@SHIPTOID*!*SHIP_TO_INTERNATIONAL@!@SHIPTOINTLFLAG*!*SHIP_TO_NAME@!@SHIPTONAME*!*SHIP_TO_ADDRESS_1@!@SHIPTOADDRESS1*!*SHIP_TO_ADDRESS_2@!@SHIPTOADDRESS2*!*SHIP_TO_ADDRESS_3@!@SHIPTOADDRESS3*!*SHIP_TO_CITY@!@SHIPTOCITY*!*SHIP_TO_STATE@!@SHIPTOSTATE*!*SHIP_TO_ZIP@!@SHIPTOZIP*!*SHIP_TO_COUNTRY@!@SHIPTOCOUNTRY*!*SHIP_TO_ATTENTION@!@SHIPTOATTENTION*!*SHIP_TO_CONTACT_NAME@!@SHIPTOCONTACTNAME*!*SHIP_TO_CONTACT_EMAIL@!@SHIPTOCONTACTEMAIL*!*SHIP_TO_CONTACT_PHONE@!@SHIPTOCONTACTPHONE*!*BILL_TO_INTERNATIONAL@!@BILLTOINTLFLAG*!*BILL_TO_NAME@!@BILLTONAME*!*BILL_TO_ADDRESS_1@!@BILLTOADDRESS1*!*BILL_TO_ADDRESS_2@!@BILLTOADDRESS2*!*BILL_TO_ADDRESS_3@!@BILLTOADDRESS3*!*BILL_TO_ADDRESS_4@!@BILLTOADDRESS4*!*BILL_TO_ADDRESS_5@!@BILLTOADDRESS5*!*BILL_TO_CITY@!@BILLTOCITY*!*BILL_TO_STATE@!@BILLTOSTATE*!*BILL_TO_ZIP@!@BILLTOZIP*!*BILL_TO_COUNTRY@!@BILLTOCOUNTRY*!*BILL_TO_ATTENTION@!@BILLTOATTENTION*!*ORDER_SORT@!@ORDERSORT*!*ORDER_SORT_2@!@ORDERSORT2*!*HEADER_COMMENTS@!@ADDLCOMMENTSHEAD*!*CUSTOMER_PO@!@CUSTOMERPO*!*UPDATE_ORDER@!@UPDATE_ORDER*!*PROSPECT@!@PROSPECT*!*ORDER_LOCATION@!@ORDERLOCATION*!*SALESPERSON_CODE@!@SALESPERSONCODE*!*TERMS@!@TERMS*!*MARK_SHIPMENT@!@MARKSHIPMENT*!*SHIP_HOLD@!@SHIPHOLD*!*QUOTE_STATUS@!@QUOTE_STATUS*!*LINE_USER_FIELD_1@!@UserField1Line*!*LINE_USER_FIELD_2@!@USERFIELD2LINE*!*LINE_USER_FIELD_3@!@USERFIELD3LINE*!*LINE_USER_FIELD_4@!@USERFIELD4LINE*!*LINE_USER_FIELD_5@!@USERFIELD5LINE*!*LINE_COMMENTS@!@ADDLCOMMENTSLINE*!*LINE_TAXES@!@LINETAXES*!*PART_UM@!@PART_UM*!*LINE_NUMBER@!@LINENUMBER*!*LINE_TYPE@!@LINETYPE*!*GSS_PART_NUMBER@!@GSS_PARTNUMBER*!*EXTERNAL_PART_NUMBER@!@EXT_PARTNUMBER*!*QUANTITY@!@QTYORDERED*!*LINE_TOTAL_PRICE@!@LINETOTALPRICE*!*LINE_UNIT_PRICE@!@LINEUNITPRICE*!*PRICE_BEFORE_DISCOUNT@!@PRICEBEFOREDISCOUNT*!*ORDER_DISCOUNT_PERCENTAGE@!@ORDERDISCPERCENTAGE*!*PART_LOCATION@!@PARTLOC*!*LINE_ORDER_DATE@!@LINEORDERDATE*!*LINE_PROMISE_DATE@!@LINEPROMISEDATE*!*FREIGHT@!@FREIGHT*!*PART_DESCRIPTION@!@PARTDESCRIPTION*!*RECORD_ID@!@RECORDID*!*LINE_UNIT_COST@!@LINEUNITCOST*!*COMMISSION_TYPE_CODE@!@COMMISSION_TYPE_CODE*!*BILL_TO_PHONE@!@BILLTOPHONE*!*SHIP_TO_ADDRESS_4@!@SHIPTOADDRESS4*!*SHIP_TO_ADDRESS_5@!@SHIPTOADDRESS5*!*FREIGHT_GL_ACCOUNT@!@FREIGHT_GL_ACCOUNT*!*DISCOUNT@!@DISCOUNT*!*FOB_POINT@!@FOB_POINT*!*DELETE_ORDER@!@DELETE_ORDER*!*NO_OVERRIDE_FREIGHT@!@NO_OVERRIDE_FREIGHT*!*CUSTOMER_PART@!@CUSTOMER_PART*!*CONFIGURATOR_PART@!@CONFIGURATOR_PART*!*PRODUCT_LINE@!@PRODUCT_LINE*!*QUANTITY_TO_PURCHASE@!@QUANTITY_TO_PURCHASE*!*ORDER_DISCOUNT@!@ORDER_DISCOUNT*!*PRICE_CLASS_DISCOUNT@!@PRICE_CLASS_DISCOUNT*!*PRODUCT_LINE_DISCOUNT@!@PRODUCT_LINE_DISCOUNT*!*APPLY_DISCOUNT@!@APPLY_DISCOUNT")
	
	F.Intrinsic.Control.CallSub(Connection, "Connect", True)
	F.Data.DataTable.SaveToDB("dtSave", "Con", "GCG_5807_ORDER_STAGE", "RECORD_ID", 256, V.Local.sMap)
	
	'V.Local.sMap.Set("EXTERNAL_ORDER_NUMBER@!@EXTERNAL_ORDER*!*REVIEW_RECORD_ID@!@RECORD_ID*!*REVIEW_RECORD_TIMESTAMP@!@RECORD_TIMESTAMP*!*REVIEW_LAST_MODIFIED_TIMESTAMP@!@LAST_MODIFIED*!*REVIEW_LAST_MODIFIED_BY@!@MODIFIED_BY*!*REVIEW_APPROVED@!@APPROVED*!*REVIEW_APPROVED_TIMESTAMP@!@APPROVED_TIMESTAMP*!*APPROVED_BY@!@APPROVED_BY")
	V.Local.sMap.Set("EXTERNAL_ORDER_NUMBER@!@EXTERNAL_ORDER*!*REVIEW_RECORD_TIMESTAMP@!@RECORD_TIMESTAMP*!*REVIEW_LAST_MODIFIED_TIMESTAMP@!@LAST_MODIFIED*!*REVIEW_LAST_MODIFIED_BY@!@MODIFIED_BY*!*REVIEW_APPROVED@!@APPROVED*!*REVIEW_APPROVED_TIMESTAMP@!@APPROVED_TIMESTAMP*!*APPROVED_BY@!@APPROVED_BY")
	F.Data.DataTable.SaveToDB("dtSaveReview", "Con", "GCG_5807_REVIEW", "EXTERNAL_ORDER_NUMBER", 256, V.Local.sMap)
	
	F.Intrinsic.Control.CallSub(Connection, "Connect", False)
	
	F.Data.DataTable.Close("dtSaveReview")
F.Intrinsic.Control.EndIf
F.Data.DataView.Close("dtSaveTemp", "dvSaveTemp")
F.Data.DataTable.Close("dtSaveTemp")

F.Intrinsic.UI.CloseWaitDialog
F.Intrinsic.Control.CallSub(ToggleScreenEnabled, "Enabled", True)
GUI.FormReviewDashboard..SetFocus

F.Intrinsic.Control.Catch
	F.Intrinsic.Control.CallSub(ErrorMessage)
F.Intrinsic.Control.EndTry
Program.Sub.cmdSave_Click.End

Program.Sub.ToggleScreenEnabled.Start
'this sub enables/disables the screen based on a passed variable
'but is meant to leave the disabled if it was already by a previous process
'pass boolean ENABLED

F.Intrinsic.Control.Try

V.Static.iDisabledCount.Declare(Long)

F.Intrinsic.Control.If(V.Args.ENABLED)
	F.Intrinsic.Control.If(V.Static.iDisabledCount, >, 0)
		F.Intrinsic.Math.Sub(V.Static.iDisabledCount, 1, V.Static.iDisabledCount)
		
		F.Intrinsic.Control.If(V.Static.iDisabledCount, =, 0)
			GUI.FormReviewDashboard..Enabled(True)
		F.Intrinsic.Control.EndIf
	F.Intrinsic.Control.EndIf
F.Intrinsic.Control.Else
	F.Intrinsic.Control.If(V.Static.iDisabledCount, =, 0)
		GUI.FormReviewDashboard..Enabled(False)
	F.Intrinsic.Control.EndIf
	
	F.Intrinsic.Math.Add(V.Static.iDisabledCount, 1, V.Static.iDisabledCount)
F.Intrinsic.Control.EndIf

F.Intrinsic.Control.Catch
	F.Intrinsic.Control.CallSub(ErrorMessage)
F.Intrinsic.Control.EndTry
Program.Sub.ToggleScreenEnabled.End

