Program.Sub.LoadOrders.Start
F.Intrinsic.Control.Try

V.Local.iCounter.Declare(Long)
V.Local.sFilter.Declare(String)
V.Local.iDataViewCounter.Declare(Long)
V.Local.sSQL.Declare(String)

F.Intrinsic.Control.CallSub(Connection, "Connect", True)
F.Data.DataTable.CreateFromSQL("dtReviewHeaderTemp", "Con", "select RTRIM(Order_No_External) EXTERNAL_ORDER_NUMBER, RTRIM(IsNull(TransactionCode, 'O')) TRANSACTION_CODE, RTRIM(GSS_CustomerNo) GSS_CUSTOMER_NUMBER, RTRIM(Ext_CustomerNo) EXTERNAL_CUSTOMER_NUMBER, RTRIM(OrderNumber) ORDER_NUMBER, RTRIM(OrderType) ORDER_TYPE, IsNull(OrderDueDate, CURDATE()) ORDER_DUE_DATE, IsNull(OrderDate, CURDATE()) ORDER_DATE, RTRIM(UserField1Head) HEADER_USER_FIELD_1, RTRIM(UserField2Head) HEADER_USER_FIELD_2, RTRIM(UserField3Head) HEADER_USER_FIELD_3, RTRIM(UserField4Head) HEADER_USER_FIELD_4, RTRIM(UserField5Head) HEADER_USER_FIELD_5, RTRIM(ShipVia) SHIP_VIA, RTRIM(Carrier) CARRIER, RTRIM(ShipToID) SHIP_TO_ID, SHipToIntlFlag SHIP_TO_INTERNATIONAL, RTRIM(ShipToName) SHIP_TO_NAME, RTRIM(ShipToAddress1) SHIP_TO_ADDRESS_1, RTRIM(ShipToAddress2) SHIP_TO_ADDRESS_2, RTRIM(ShipToAddress3) SHIP_TO_ADDRESS_3, RTRIM(ShipToCity) SHIP_TO_CITY, RTRIM(ShipToState) SHIP_TO_STATE, RTRIM(IsNull(ShipToZip, '')) SHIP_TO_ZIP, RTRIM(ShipToCountry) SHIP_TO_COUNTRY, RTRIM(ShipToAttention) SHIP_TO_ATTENTION, RTRIM(ShipToContactName) SHIP_TO_CONTACT_NAME, RTRIM(ShipToContactEmail) SHIP_TO_CONTACT_EMAIL, RTRIM(ShipToContactPhone) SHIP_TO_CONTACT_PHONE, BillToIntlFlag BILL_TO_INTERNATIONAL, RTRIM(BillToName) BILL_TO_NAME, RTRIM(BillToAddress1) BILL_TO_ADDRESS_1, RTRIM(BillToAddress2) BILL_TO_ADDRESS_2, RTRIM(BillToAddress3) BILL_TO_ADDRESS_3, RTRIM(BillToAddress4) BILL_TO_ADDRESS_4, RTRIM(BillToAddress5) BILL_TO_ADDRESS_5, RTRIM(BillToCity) BILL_TO_CITY, RTRIM(BillToState) BILL_TO_STATE, RTRIM(BillToZip) BILL_TO_ZIP, RTRIM(BillToCountry) BILL_TO_COUNTRY, RTRIM(BillToAttention) BILL_TO_ATTENTION, RTRIM(OrderSort) ORDER_SORT, RTRIM(OrderSort2) ORDER_SORT_2, RTRIM(AddlCommentsHead) HEADER_COMMENTS, RTRIM(CustomerPO) CUSTOMER_PO, A.PROCESSED, FAILED, RTRIM(FeedBack) FEEDBACK, UPDATE_ORDER, PROSPECT, RTRIM(OrderLocation) ORDER_LOCATION, RTRIM(SalesPersonCode) SALESPERSON_CODE, A.RECORD_TIMESTAMP, RTRIM(Terms) TERMS, RTRIM(MarkShipment) MARK_SHIPMENT, SHIPHOLD SHIP_HOLD, RTRIM(QUOTE_STATUS) QUOTE_STATUS, LineNumber LINE_NUMBER, B.RECORD_ID REVIEW_RECORD_ID, B.RECORD_TIMESTAMP REVIEW_RECORD_TIMESTAMP, B.LAST_MODIFIED REVIEW_LAST_MODIFIED_TIMESTAMP, RTRIM(B.MODIFIED_BY) REVIEW_LAST_MODIFIED_BY, B.APPROVED REVIEW_APPROVED, B.APPROVED_TIMESTAMP REVIEW_APPROVED_TIMESTAMP, RTRIM(B.APPROVED_BY) APPROVED_BY, B.PROCESSED REVIEW_PROCESSED, RTRIM(A.COMMISSION_TYPE_CODE) COMMISSION_TYPE_CODE, A.FREIGHT, A.PROCESSED_TIMESTAMP PROCESSED_TIMESTAMP, A.BILLTOPHONE BILL_TO_PHONE, RTRIM(ShipToAddress4) SHIP_TO_ADDRESS_4, RTRIM(SHIPTOADDRESS5) SHIP_TO_ADDRESS_5, RTRIM(FREIGHT_GL_ACCOUNT) FREIGHT_GL_ACCOUNT, DISCOUNT, RTRIM(FOB_POINT) FOB_POINT, DELETE_ORDER, NO_OVERRIDE_FREIGHT from GCG_5807_ORDER_STAGE A left join GCG_5807_REVIEW B on A.ORDER_NO_EXTERNAL = B.EXTERNAL_ORDER and A.PROCESSED = B.PROCESSED where A.PROCESSED = 0 or FAILED = 1 order by A.RECORD_TIMESTAMP", True)

'need to make sure this is distinct
F.Data.DataView.Create("dtReviewHeaderTemp", "dvReviewHeaderTemp", 22, "", "LINE_NUMBER")
F.Data.DataView.ToDataTableDistinct("dtReviewHeaderTemp", "dvReviewHeaderTemp", "dtOrdersDistinct", "EXTERNAL_ORDER_NUMBER")

F.Intrinsic.Control.For(V.Local.iCounter, 0, V.DataTable.dtOrdersDistinct.RowCount--, 1)
	F.Intrinsic.String.Build("EXTERNAL_ORDER_NUMBER = '{0}'", V.DataTable.dtOrdersDistinct(V.Local.iCounter).EXTERNAL_ORDER_NUMBER!FieldValTrim, V.Local.sFilter)
	F.Data.DataView.SetFilter("dtReviewHeaderTemp", "dvReviewHeaderTemp", V.Local.sFilter)
	
	F.Intrinsic.Control.If(V.DataView.dtReviewHeaderTemp!dvReviewHeaderTemp.RowCount, >, 1)
		F.Intrinsic.Control.For(V.Local.iDataViewCounter, V.DataView.dtReviewHeaderTemp!dvReviewHeaderTemp.RowCount--, 1, -1)
			F.Data.DataView.DeleteRow("dtReviewHeaderTemp", "dvReviewHeaderTemp", V.Local.iDataViewCounter)
		F.Intrinsic.Control.Next(V.Local.iDataViewCounter)
	F.Intrinsic.Control.EndIf
	
F.Intrinsic.Control.Next(V.Local.iCounter)
F.Data.DataTable.AcceptChanges("dtReviewHeaderTemp")

F.Intrinsic.Control.CallSub(Connection, "Connect", False)

