Program.Sub.DeleteOrder.Start
'deletes an order from the order staging table
'pass EXTERNAL_ORDER, pass FAILED

F.Intrinsic.Control.Try

V.Local.sFilter.Declare(String)
V.Local.sSQL.Declare(String)

F.Intrinsic.Control.If(V.Args.Failed)
	V.Local.sFilter.Set("and PROCESSED = 1 and FAILED = 1")
F.Intrinsic.Control.Else
	V.Local.sFilter.Set("and PROCESSED = 0 and FAILED = 0")
F.Intrinsic.Control.EndIf

F.Intrinsic.String.Build("delete from GCG_5807_ORDER_STAGE where ORDER_NO_EXTERNAL = '{0}' {1}", V.Args.External_Order, V.Local.sFilter, V.Local.sSQL)

F.Intrinsic.Control.CallSub(Connection, "Connect", True)
F.ODBC.Connection!Con.Execute(V.Local.sSQL)
F.Intrinsic.Control.CallSub(Connection, "Connect", False)

F.Intrinsic.Control.Catch
	F.Intrinsic.Control.CallSub(ErrorMessage)
F.Intrinsic.Control.EndTry
Program.Sub.DeleteOrder.End

