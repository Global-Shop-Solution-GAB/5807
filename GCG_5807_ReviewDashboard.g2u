V.Local.iRet.Declare(Long)
V.Local.sSQL.Declare(String)
V.Local.sFilter.Declare(String)
V.Local.sPath.Declare(String)
V.Local.bFailed.Declare(Boolean)
V.Local.sDataTable.Declare(String)
V.Local.sMessage.Declare(String)
V.Local.sRet.Declare(String)
V.Local.iFailed.Declare(Long)
V.Local.iSuccess.Declare(Long)
V.Local.sFeedback.Declare(String)
V.Local.iCounter.Declare(Long)
V.Local.sType.Declare(String)

F.Intrinsic.Control.If(V.Args.ControlName.UCase, =, "CMDUPLOAD")
	V.Local.bFailed.Set(False)
	V.Local.sDataTable.Set("dtReviewHeader")
F.Intrinsic.Control.Else
	V.Local.bFailed.Set(True)
	V.Local.sDataTable.Set("dtFailedHeader")
F.Intrinsic.Control.EndIf

F.Data.DataView.Create(V.Local.sDataTable, "dvUpload", 22, "[SELECTED] = True", "")
F.Intrinsic.Control.If(V.DataView.[V.Local.sDataTable]!dvUpload.RowCount, >, 0)
	F.Intrinsic.UI.Msgbox("Mark the selected orders as approved and upload?", "Upload", 4, V.Local.iRet)
	F.Intrinsic.Control.If(V.Local.iRet, =, V.Ambient.MsgBoxYes)
		F.Intrinsic.Control.CallSub(ToggleScreenEnabled, "Enabled", False)
		
		F.Intrinsic.Control.CallSub(cmdSave_Click)
		
		F.Intrinsic.UI.InvokeWaitDialog("Preparing for upload....")
		
		F.Intrinsic.Control.CallSub(Connection, "Connect", True)
		
		F.Data.DataView.ToString(V.Local.sDataTable, "dvUpload", "EXTERNAL_ORDER_NUMBER", "***NO-INNER-DELIMITER***", "*!*", V.Local.sFilter)
		
		'should not be any duplicates, but it never hurts to check
		F.Intrinsic.String.Split(V.Local.sFilter, "*!*", V.Local.sFilter)
		F.Intrinsic.String.RemoveArrayDuplicates(V.Local.sFilter, V.Local.sFilter)
		F.Intrinsic.String.Join(V.Local.sFilter, "','", V.Local.sFilter)
		V.Local.sFilter.RedimPreserve(0, 0)
		
		'mark records as approved, ready for upload
		F.Intrinsic.Control.If(V.Local.bFailed)
			F.Intrinsic.String.Build("update GCG_5807_ORDER_STAGE set APPROVED = 1, PROCESSED = 0, Feedback = 'DASHBOARD-UPLOAD', FAILED = 0 where ORDER_NO_EXTERNAL in ('{0}') and PROCESSED = 1", V.Local.sFilter, V.Local.sSQL)
		F.Intrinsic.Control.Else
			F.Intrinsic.String.Build("update GCG_5807_ORDER_STAGE set APPROVED = 1, Feedback = 'DASHBOARD-UPLOAD' where ORDER_NO_EXTERNAL in ('{0}') and PROCESSED = 0", V.Local.sFilter, V.Local.sSQL)
		F.Intrinsic.Control.EndIf
		F.ODBC.Connection!Con.Execute(V.Local.sSQL)
		
		'also update approved table
		F.Intrinsic.Control.If(V.Local.bFailed)
			F.Intrinsic.String.Build("select * from GCG_5807_REVIEW where EXTERNAL_ORDER in ('{0}') and PROCESSED = 1", V.Local.sFilter, V.Local.sSQL)
		F.Intrinsic.Control.Else
			F.Intrinsic.String.Build("select * from GCG_5807_REVIEW where EXTERNAL_ORDER in ('{0}') and PROCESSED = 0", V.Local.sFilter, V.Local.sSQL)
		F.Intrinsic.Control.EndIf
		F.Data.DataTable.CreateFromSQL("dtApprovedSave", "Con", V.Local.sSQL)
		F.Data.DataTable.SetValue("dtApprovedSave", -1, "APPROVED", True, "APPROVED_TIMESTAMP", V.Ambient.Now, "APPROVED_BY", V.Caller.User)
		F.Data.DataTable.SaveToDB("dtApprovedSave", "Con", "GCG_5807_REVIEW", "RECORD_ID", 256)
		F.Data.DataTable.Close("dtApprovedSave")
		
		'now run the uploader
		'F.Intrinsic.UI.ChangeWaitStatus("Running uploader....")
		F.Intrinsic.UI.CloseWaitDialog
		F.Intrinsic.String.Build("{0}\GCG_5807_OrderUploader.g2u", V.Ambient.ScriptPath, V.Local.sPath)
		F.Global.General.CallSyncGAS(V.Local.sPath, V.Caller.Switches)
		
		'fix any orders left queued if there was a crash
		F.ODBC.Connection!Con.Execute("update GCG_5807_ORDER_STAGE set FEEDBACK = REPLACE(FEEDBACK, 'DASHBOARD-UPLOAD', '') where PROCESSED = 0;")
		
		'clear lock file, in case the program crashed before clearing it
		F.Intrinsic.Control.CallSub(ClearUploaderLock, "Dashboard", True)
		
		'F.Intrinsic.UI.CloseWaitDialog
		
		'check orders
		F.Intrinsic.String.Build("select count(distinct ORDER_NO_EXTERNAL) from GCG_5807_ORDER_STAGE where ORDER_NO_EXTERNAL in ('{0}') and PROCESSED = 1 and FAILED = 0 and PROCESSED_TIMESTAMP > DATEADD(MINUTE, -30, NOW())", V.Local.sFilter, V.Local.sSQL)
		F.ODBC.Connection!Con.ExecuteAndReturn(V.Local.sSQL, V.Local.sRet)
		F.Intrinsic.Control.If(V.Ambient.ExecuteAndReturnEOF)
			V.Local.iSuccess.Set(0)
		F.Intrinsic.Control.Else
			V.Local.iSuccess.Set(V.Local.sRet.Long)
		F.Intrinsic.Control.EndIf
		
		F.Intrinsic.String.Build("select count(distinct ORDER_NO_EXTERNAL) from GCG_5807_ORDER_STAGE where ORDER_NO_EXTERNAL in ('{0}') and PROCESSED = 1 and FAILED = 1 and PROCESSED_TIMESTAMP > DATEADD(MINUTE, -30, NOW())", V.Local.sFilter, V.Local.sSQL)
		F.ODBC.Connection!Con.ExecuteAndReturn(V.Local.sSQL, V.Local.sRet)
		F.Intrinsic.Control.If(V.Ambient.ExecuteAndReturnEOF)
			V.Local.iFailed.Set(0)
		F.Intrinsic.Control.Else
			V.Local.iFailed.Set(V.Local.sRet.Long)
		F.Intrinsic.Control.EndIf
		
		F.Intrinsic.Control.If(V.Local.iSuccess, =, 0, "AND", V.Local.iFailed, =, 0)
			V.Local.sMessage.Set("The order uploader failed during the upload process. Some orders may have still been uploaded. Please refresh the dashboard to check which orders failed and which are still waiting to be uploaded.")
		F.Intrinsic.Control.Else
			F.Intrinsic.String.Build("Upload process complete. {0} order(s) uploaded, {1} order(s) failed.", V.Local.iSuccess, V.Local.iFailed, V.Local.sMessage)
		F.Intrinsic.Control.EndIf
		
		F.Intrinsic.String.Build("select distinct ORDER_NO_EXTERNAL, FAILED, FEEDBACK, ORDERNUMBER, TRANSACTIONCODE from GCG_5807_ORDER_STAGE where ORDER_NO_EXTERNAL in ('{0}') and PROCESSED = 1 and PROCESSED_TIMESTAMP > DATEADD(MINUTE, -30, NOW()) order by FAILED, ORDERNUMBER", V.Local.sFilter, V.Local.sSQL)
		F.Data.DataTable.CreateFromSQL("dtFeedback", "Con", V.Local.sSQL)
		F.Intrinsic.Control.CallSub(Connection, "Connect", False)
		
		F.Intrinsic.StringBuilder.Create("FeedbackString")
		F.Intrinsic.Control.For(V.Local.iCounter, 0, V.DataTable.dtFeedback.RowCount--, 1)
			F.Intrinsic.Control.If(V.DataTable.dtFeedback(V.Local.iCounter).TRANSACTIONCODE!FieldValTrim, =, Q)
				V.Local.sType.Set("Quote")
			F.Intrinsic.Control.Else
				V.Local.sType.Set("Order")
			F.Intrinsic.Control.EndIf
			
			F.Intrinsic.Control.If(V.DataTable.dtFeedback(V.Local.iCounter).FAILED!FieldVal)
				F.Intrinsic.String.Build("{0} with external ID {1} failed to upload.", V.Local.sType, V.DataTable.dtFeedback(V.Local.iCounter).ORDER_NO_EXTERNAL!FieldValTrim, V.Local.sFeedback)
			F.Intrinsic.Control.Else
				F.Intrinsic.String.Build("{0} with external ID {1} uploaded successfully as {2} {3}.", V.Local.sType, V.DataTable.dtFeedback(V.Local.iCounter).ORDER_NO_EXTERNAL!FieldValTrim, V.Local.sType.LCase, V.DataTable.dtFeedback(V.Local.iCounter).ORDERNUMBER!FieldValTrim, V.Local.sFeedback)
			F.Intrinsic.Control.EndIf
			
			F.Intrinsic.StringBuilder.AppendLine("FeedbackString", V.Local.sFeedback)
		F.Intrinsic.Control.Next(V.Local.iCounter)
		F.Intrinsic.StringBuilder.ToString("FeedbackString", V.Local.sFeedback)
		F.Intrinsic.StringBuilder.Dispose("FeedbackString")
		F.Data.DataTable.Close("dtFeedback")
		
		F.Intrinsic.String.Build("{0}{1}{1}{2}", V.Local.sMessage, V.Ambient.NewLine, V.Local.sFeedback, V.Local.sMessage)
		F.Intrinsic.UI.Msgbox(V.Local.sMessage, "5807 Review Dashboard")
		
		F.Intrinsic.Control.CallSub(cmdRefresh_Click)
		F.Intrinsic.Control.CallSub(ToggleScreenEnabled, "Enabled", True)
	F.Intrinsic.Control.EndIf
F.Intrinsic.Control.Else
	F.Intrinsic.UI.Msgbox("No records selected for upload.")
F.Intrinsic.Control.EndIf
F.Data.DataView.Close(V.Local.sDataTable, "dvUpload")

F.Intrinsic.Control.Catch
	F.Intrinsic.Control.CallSub(ErrorMessage)
F.Intrinsic.Control.EndTry
Program.Sub.cmdUpload_Click.End

Program.Sub.cmdSave_Click.Start
F.Intrinsic.Control.Try

V.Local.sMap.Declare(String)

F.Intrinsic.UI.InvokeWaitDialog("Saving...")
F.Intrinsic.Control.CallSub(ToggleScreenEnabled, "Enabled", False)

F.Data.DataTable.AcceptChanges("dtReviewHeader")
F.Data.DataTable.AcceptChanges("dtReviewHeader$dtReviewLines")
F.Data.DataTable.AcceptChanges("dtFailedHeader")
F.Data.DataTable.AcceptChanges("dtFailedHeader$dtFailedLines")

F.Data.Linq.Join("LeftJoin", "DataTable", "dtReviewHeader*!*A", "DataTable", "dtReviewHeader$dtReviewLines*!*B", "A.EXTERNAL_ORDER_NUMBER = B.EXTERNAL_ORDER_NUMBER_LINE", "*", "", "", "", "dtSaveTemp", True)
F.Data.Linq.Join("LeftJoin", "DataTable", "dtFailedHeader*!*A", "DataTable", "dtFailedHeader$dtFailedLines*!*B", "A.EXTERNAL_ORDER_NUMBER = B.EXTERNAL_ORDER_NUMBER_LINE", "*", "", "", "", "dtSaveTempFailed", True)
F.Data.DataTable.Merge("dtSaveTempFailed", "dtSaveTemp", False, 2)
F.Data.DataTable.Close("dtSaveTempFailed")

F.Data.DataView.Create("dtSaveTemp", "dvSaveTemp", 22, "[MODIFIED] = True", "")
